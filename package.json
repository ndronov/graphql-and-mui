{
  "name": "graphql-and-mui",
  "description": "GraphQL and Material UI demo project",
  "private": true,
  "version": "1.0.0",
  "author": "Nikita Dronov",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "prettier": "prettier \"src/**/*.{ts,tsx}\"",
    "prettier:check": "yarn prettier --list-different",
    "prettier:fix": "yarn prettier --write --list-different",
    "lint:scripts": "eslint \"src/**/*.{ts,tsx}\" --config .eslintrc.cjs",
    "lint:scripts:fix": "eslint \"src/**/*.{ts,tsx}\" --config .eslintrc.cjs --fix",
    "lint:types": "tsc",
    "lint:all": "yarn prettier:check && yarn lint:scripts && yarn lint:types",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@emotion/react": "^11.11.3",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@emotion/styled": "^11.11.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.4",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.4",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.1",
    "prettier": "^3.0.1",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vite-aliases": "^0.11.2"
  }
}
